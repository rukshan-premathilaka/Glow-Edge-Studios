{
    "name": "rasintha_rukshan/glow-edge-studios",
    "description": "This project is an online booking website for a company that offers graphic designing and photography services. The main purpose of the website is to allow customers to book a graphic designer or photographer online, quickly and easily.",
    "type": "2",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "controller\\": "src/controller/",
            "core\\": "src/core/",
            "csrf\\": "src/csrf/"
        }
    },
    "authors": [
        {
            "name": "rukshan-prog",
            "email": "rukshanprog@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "phroute/phroute": "^2.2",
        "respect/validation": "^2.4",
      "ext-pdo": "*",
        "phpmailer/phpmailer": "^6.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "roave/security-advisories": "dev-latest"

    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
